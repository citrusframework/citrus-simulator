{
  "name": "citrus-simulator",
  "version": "0.0.0",
  "private": true,
  "description": "Description for Citrus Simulator",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "build": "npm run webapp:prod --",
    "build-watch": "concurrently 'npm run webapp:build:dev -- --watch'",
    "ci:frontend:build": "npm run webapp:build:$npm_package_config_default_environment",
    "ci:frontend:test": "npm run ci:frontend:build && npm test",
    "clean-www": "rimraf target/classes/static/simulator-ui/app/{src,target/}",
    "cleanup": "rimraf target/classes/static/simulator-ui/",
    "e2e": "playwright test",
    "e2e:open": "npm run e2e -- --open",
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --config jest.conf.js",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prettier:check": "prettier --check \"{,src/**/,webpack/,.blueprint/**/,./test/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss}\"",
    "prettier:format": "prettier --write \"{,src/**/,webpack/,.blueprint/**/,./test/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss}\"",
    "serve": "npm run start --",
    "start": "ng serve --hmr",
    "start-tls": "npm run webapp:dev-ssl",
    "pretest": "npm run lint",
    "test": "ng test --coverage --log-heap-usage -w=2",
    "test:watch": "npm run test -- --watch",
    "watch": "concurrently npm:start",
    "webapp:build": "npm run clean-www && npm run webapp:build:dev",
    "webapp:build:dev": "ng build --configuration development",
    "webapp:build:prod": "ng build --configuration production",
    "webapp:dev": "ng serve",
    "webapp:dev-ssl": "ng serve --ssl",
    "webapp:dev-verbose": "ng serve --verbose",
    "webapp:prod": "npm run clean-www && npm run webapp:build:prod",
    "webapp:test": "npm run test --"
  },
  "config": {
    "backend_port": 8080,
    "default_environment": "prod"
  },
  "overrides": {
    "browser-sync": "3.0.4",
    "webpack": "5.103.0"
  },
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/localize": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@fortawesome/angular-fontawesome": "3.0.0",
    "@fortawesome/fontawesome-svg-core": "7.1.0",
    "@fortawesome/free-brands-svg-icons": "7.1.0",
    "@fortawesome/free-solid-svg-icons": "7.1.0",
    "@ng-bootstrap/ng-bootstrap": "^19.0.0",
    "@ngx-translate/core": "17.0.0",
    "@ngx-translate/http-loader": "17.0.0",
    "axios": "1.12.0",
    "bootstrap": "5.3.7",
    "bootswatch": "5.3.7",
    "dayjs": "1.11.13",
    "mdb-angular-ui-kit": "^9.0.0",
    "ngx-highlightjs": "12.0.0",
    "ngx-infinite-scroll": "^20.0.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^20.0.0",
    "@angular-builders/jest": "^20.0.0",
    "@angular-devkit/build-angular": "^20.0.0",
    "@angular-eslint/eslint-plugin": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/service-worker": "^20.0.0",
    "@playwright/test": "1.55.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.17.2",
    "angular-eslint": "20.6.0",
    "browser-sync": "3.0.4",
    "browser-sync-v3-webpack-plugin": "0.1.0",
    "buffer": "6.0.3",
    "concurrently": "9.2.0",
    "copy-webpack-plugin": "12.0.2",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "folder-hash": "4.1.1",
    "generator-jhipster": "8.11.0",
    "jest": "29.7.0",
    "jest-date-mock": "1.0.10",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-preset-angular": "14.6.1",
    "jest-sonar": "0.2.16",
    "jiti": "2.6.1",
    "merge-jsons-webpack-plugin": "2.0.1",
    "prettier": "3.7.4",
    "prettier-plugin-packagejson": "2.5.20",
    "rimraf": "6.0.1",
    "swagger-ui-dist": "5.27.1",
    "ts-jest": "29.4.1",
    "typescript": ">=5.8",
    "typescript-eslint": "8.48.1",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-merge": "6.0.1",
    "webpack-notifier": "1.15.0",
    "zone.js": "0.15.1"
  },
  "optionalDependencies": {
    "npm-check-updates": "18.0.2"
  },
  "engines": {
    "node": ">=24.11.1"
  }
}

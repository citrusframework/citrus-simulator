{
  "name": "citrus-simulator",
  "version": "0.0.0",
  "private": true,
  "description": "Citrus Simulator UI",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run webapp:prod --",
    "build-watch": "concurrently 'npm run webapp:build:dev -- --watch'",
    "ci:frontend:build": "npm run webapp:build:$npm_package_config_default_environment",
    "ci:frontend:test": "npm run ci:frontend:build && npm test",
    "clean-www": "rimraf target/classes/static/",
    "cleanup": "rimraf target/",
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --config jest.conf.js",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prettier:check": "prettier --check \"{,src/**/,webpack/,.blueprint/**/,./test/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss}\"",
    "prettier:format": "prettier --write \"{,src/**/,webpack/,.blueprint/**/,./test/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss}\"",
    "serve": "npm run start --",
    "sonar": "sonar-scanner",
    "start": "ng serve --hmr",
    "start-tls": "npm run webapp:dev-ssl",
    "pretest": "npm run lint",
    "test": "ng test --coverage --log-heap-usage -w=2",
    "test:watch": "npm run test -- --watch",
    "watch": "concurrently npm:start",
    "webapp:build": "npm run clean-www && npm run webapp:build:dev",
    "webapp:build:dev": "ng build --configuration development",
    "webapp:build:prod": "ng build --configuration production",
    "webapp:dev": "ng serve",
    "webapp:dev-ssl": "ng serve --ssl",
    "webapp:dev-verbose": "ng serve --verbose",
    "webapp:prod": "npm run clean-www && npm run webapp:build:prod",
    "webapp:test": "npm run test --"
  },
  "config": {
    "default_environment": "prod"
  },
  "dependencies": {
    "@angular/common": "19.2.14",
    "@angular/compiler": "19.2.14",
    "@angular/core": "19.2.14",
    "@angular/forms": "19.2.14",
    "@angular/localize": "19.2.14",
    "@angular/platform-browser": "19.2.14",
    "@angular/platform-browser-dynamic": "19.2.14",
    "@angular/router": "19.2.14",
    "@fortawesome/angular-fontawesome": "1.0.0",
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-brands-svg-icons": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@ng-bootstrap/ng-bootstrap": "18.0.0",
    "@ngx-translate/core": "17.0.0",
    "@ngx-translate/http-loader": "17.0.0",
    "@popperjs/core": "2.11.8",
    "axios": "1.11.0",
    "bootstrap": "5.3.7",
    "bootswatch": "5.3.7",
    "dayjs": "1.11.13",
    "highlight.js": "11.11.1",
    "mdb-angular-ui-kit": "8.0.0",
    "ngx-infinite-scroll": "19.0.0",
    "prettier": "3.6.2",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "19.0.1",
    "@angular-builders/jest": "19.0.1",
    "@angular-devkit/build-angular": "19.2.15",
    "@angular/cli": "19.2.15",
    "@angular/compiler-cli": "19.2.14",
    "@angular/service-worker": "19.2.14",
    "@eslint/js": "9.34.0",
    "@playwright/test": "1.55.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.3.0",
    "angular-eslint": "19.8.1",
    "browser-sync": "3.0.4",
    "browser-sync-v3-webpack-plugin": "0.1.0",
    "buffer": "6.0.3",
    "concurrently": "9.2.0",
    "copy-webpack-plugin": "13.0.1",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-webpack-plugin": "5.0.2",
    "folder-hash": "4.1.1",
    "generator-jhipster": "8.11.0",
    "globals": "16.3.0",
    "jest": "29.7.0",
    "jest-date-mock": "1.0.10",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-preset-angular": "14.5.5",
    "jest-sonar": "0.2.16",
    "merge-jsons-webpack-plugin": "2.0.1",
    "prettier-plugin-packagejson": "2.5.19",
    "rimraf": "6.0.1",
    "swagger-ui-dist": "5.27.1",
    "ts-jest": "29.4.1",
    "typescript": ">=5.5",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-merge": "6.0.1",
    "webpack-notifier": "1.15.0"
  },
  "optionalDependencies": {
    "npm-check-updates": "18.0.2"
  },
  "engines": {
    "node": ">=22.18.0 || >=24.6.0"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}

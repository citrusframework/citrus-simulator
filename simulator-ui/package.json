{
  "name": "citrus-simulator",
  "version": "0.0.0",
  "private": true,
  "description": "Description for Citrus Simulator",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run webapp:prod --",
    "build-watch": "concurrently 'npm run webapp:build:dev -- --watch'",
    "ci:frontend:build": "npm run webapp:build:$npm_package_config_default_environment",
    "ci:frontend:test": "npm run ci:frontend:build && npm test",
    "clean-www": "rimraf target/classes/static/simulator-ui/app/{src,target/}",
    "cleanup": "rimraf target/classes/static/simulator-ui/",
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --config jest.conf.js",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier:check": "prettier --check \"{,src/**/,webpack/,.blueprint/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss}\"",
    "prettier:format": "prettier --write \"{,src/**/,webpack/,.blueprint/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss}\"",
    "serve": "npm run start --",
    "start": "ng serve --hmr",
    "start-tls": "npm run webapp:dev-ssl",
    "pretest": "npm run lint",
    "test": "ng test --coverage --log-heap-usage -w=2",
    "test:watch": "npm run test -- --watch",
    "watch": "concurrently npm:start",
    "webapp:build": "npm run clean-www && npm run webapp:build:dev",
    "webapp:build:dev": "ng build --configuration development",
    "webapp:build:prod": "ng build --configuration production",
    "webapp:dev": "ng serve",
    "webapp:dev-ssl": "ng serve --ssl",
    "webapp:dev-verbose": "ng serve --verbose",
    "webapp:prod": "npm run clean-www && npm run webapp:build:prod",
    "webapp:test": "npm run test --"
  },
  "config": {
    "default_environment": "prod"
  },
  "dependencies": {
    "@angular/common": "17.1.1",
    "@angular/compiler": "17.1.1",
    "@angular/core": "17.1.1",
    "@angular/forms": "17.1.1",
    "@angular/localize": "17.1.1",
    "@angular/platform-browser": "17.1.1",
    "@angular/platform-browser-dynamic": "17.1.1",
    "@angular/router": "17.1.1",
    "@fortawesome/angular-fontawesome": "0.14.1",
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@ng-bootstrap/ng-bootstrap": "16.0.0",
    "@ngx-translate/core": "15.0.0",
    "@ngx-translate/http-loader": "8.0.0",
    "@popperjs/core": "2.11.8",
    "bootstrap": "5.3.2",
    "bootswatch": "5.3.2",
    "dayjs": "1.11.10",
    "ngx-infinite-scroll": "17.0.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "17.0.0",
    "@angular-builders/jest": "17.0.0",
    "@angular-devkit/build-angular": "17.2.0",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular/cli": "17.1.1",
    "@angular/compiler-cli": "17.1.1",
    "@angular/service-worker": "17.1.1",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.9",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "browser-sync": "3.0.2",
    "browser-sync-v3-webpack-plugin": "^0.1.0",
    "buffer": "6.0.3",
    "concurrently": "8.2.2",
    "copy-webpack-plugin": "12.0.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-webpack-plugin": "4.0.1",
    "folder-hash": "4.0.4",
    "generator-jhipster": "8.1.0",
    "jest": "29.7.0",
    "jest-date-mock": "1.0.8",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-preset-angular": "14.0.0",
    "jest-sonar": "0.2.16",
    "merge-jsons-webpack-plugin": "2.0.1",
    "prettier": "3.2.4",
    "prettier-plugin-packagejson": "2.4.9",
    "rimraf": "5.0.5",
    "swagger-ui-dist": "5.11.1",
    "ts-jest": "29.1.2",
    "typescript": "^5.3",
    "wait-on": "7.2.0",
    "webpack-bundle-analyzer": "4.10.1",
    "webpack-merge": "5.10.0",
    "webpack-notifier": "1.15.0"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "overrides": {
    "webpack": "5.88.2"
  }
}
